    "node-sass": "^9.0.0",
    "sass-loader": "^10.5.2",
    "stylelint": "^13.0.0",
    "stylelint-config-recommended-scss": "^4.0.0",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.0.0"
  }
