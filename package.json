    "nuxt": "2.18.1",
    "portal-vue": "^1.3.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "resize-observer": "^1.0.0",
    "source-map-support": "^0.5.4",
    "swiper": "^4.5.0",
    "tiny-cookie": "^2.3.1",
    "url-parse": "^1.5.9",
    "uuid": "^3.2.1",
    "vue-clipboard2": "^0.2.1",
    "vue-cookie": "^1.1.4",
    "vue-gtag": "^1.16.1",
