    "multer": "^1.3.0",
    "nuxt": "^2.3.4",
    "oauth-1.0a": "^2.2.4",
    "portal-vue": "^1.3.0",
    "sharp": "^0.20.0",
    "source-map-support": "^0.5.4",
    "url-parse": "^1.4.3",
    "uuid": "^3.2.1",
    "vue-awesome-swiper": "^3.1.3",
    "vue-clipboard2": "^0.2.1",
    "vue-cookie": "^1.1.4",
    "vue-i18n": "^8.0.0",
    "vue-intercom": "^1.0.12",
