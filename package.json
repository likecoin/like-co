    "express": "^4.16.3",
    "express-jwt": "^5.3.1",
    "express-rate-limit": "^3.1.0",
    "firebase": "^5.7.0",
    "firebase-admin": "^6.4.0",
    "form-data": "^2.3.2",
    "http-proxy-middleware": "^0.19.1",
    "i18n": "^0.8.3",
