    "@likecoin/ui-vue": "^0.6.0",
    "@likecoin/vue-image-lightbox": "^5.5.4",
    "@likecoin/vue-simple-svg": "^1.4.1",
    "@nuxtjs/google-tag-manager": "^2.1.1",
    "@nuxtjs/sentry": "^2.1.0",
    "aws-sdk": "^2.224.1",
    "axios": "^0.18.0",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.2",
    "classlist-polyfill": "^1.2.0",
